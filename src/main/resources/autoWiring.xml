<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">
    
    <bean id = "employee" class = "com.example.demo.settergetter.Employee" autowire = "byName" >
    <property name = "employeeName" value = "Auro wiring using constructor DI"></property>
    <property name = "age" value = "44"></property>
    <property name = "employeeLastName" value = "wiring"></property>
    
   </bean>
   
   <bean id = "address" class = "com.example.demo.settergetter.Address">
   <property name = "address1" value = "Auto wiring"> </property>
   <property name = "address2" value = ""> </property>
   <property name = "pinCode" value = "983279"> </property>
   </bean>
    </beans>